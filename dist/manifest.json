{
  "name": "RingCentral Embeddable Voice for HubSpot",
  "description": "RingCentral Embeddable Voice for HubSpot",
  "version": "0.0.1",
  "permissions": [
    "http://*/",
    "https://*/",
    "storage",
    "activeTab",
    "tabs",
    "background",
    "tabCapture",
    "unlimitedStorage",
    "https://*.hubspot.com/*",
    "https://*.hubspot.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://app.hubspot.com/contacts*"
      ],
      "js": [
        "./content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {},
  "content_security_policy": "script-src 'self' https://apps.ringcentral.com/integration/ringcentral-embeddable/0.1.0/index.html; object-src 'self'",
  "manifest_version": 2
}